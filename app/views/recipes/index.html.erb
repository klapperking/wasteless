<h1>My recipes</h1>

<% if @match_order %>
  <% @match_order.each do |recipe, count| %>
    <div class= "m-3">
      <%= render partial: "shared/recipe_card", locals: { recipe: recipe } %>
    </div>
  <% end %>
<% else %>
  <% @recipes.each do |recipe| %>
      <div class= "m-3">
        <%= render partial: "shared/recipe_card", locals: { recipe: recipe } %>
      </div>
  <% end %>
<% end %>
